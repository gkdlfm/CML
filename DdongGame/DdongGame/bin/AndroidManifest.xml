<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ddonggame"
    android:versionCode="1"
    android:versionName="1.0" >
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="kr.co.ddonggame.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kr.co.ddonggame.MainMenu"
            android:label="@string/title_activity_main_menu"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="kr.co.ddonggame.game.GameActivity"
            android:label="@string/title_activity_game"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="kr.co.ddonggame.GameRoom"
            android:label="@string/title_activity_game_room"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="kr.co.ddonggame.game.RoomEnter"
            android:label="@string/title_activity_game"
            android:screenOrientation="landscape" >
        </activity>
    </application>
    
    

</manifest>
